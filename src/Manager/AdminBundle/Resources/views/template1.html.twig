<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>Shop Manager</title>
        {% block stylesheets %}
        <link href="{{ asset('/bundles/manageradmin/css/T/templatemo_style.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('/bundles/manageradmin/css/T/coda-slider.css') }}" rel="stylesheet" type="text/css" />
         {% endblock %}
        
      
        {% block javascripts %}
        <script src="{{asset('/bundles/manageradmin/js/jquery-1.2.6.js')}}" type="text/javascript"></script>
        <script src="{{asset('/bundles/manageradmin/js/jquery.scrollTo-1.3.3.js')}}" type="text/javascript"></script>
        <script src="{{asset('/bundles/manageradmin/js/jquery.localscroll-1.2.5.js')}}" type="text/javascript" charset="utf-8"></script>
        <script src="{{asset('/bundles/manageradmin/js/jquery.serialScroll-1.2.1.js')}}" type="text/javascript" charset="utf-8"></script>
        <script src="{{asset('/bundles/manageradmin/js/coda-slider.js')}}" type="text/javascript" charset="utf-8"></script>
        <script src="{{asset('/bundles/manageradmin/js/jquery.easing.1.3.js')}}" type="text/javascript" charset="utf-8"></script>
        {% endblock %}
    </head>
    <body>

        <div id="slider">

            <div id="templatemo_sidebar">
                <div id="templatemo_header">
                    <a href="{{ path('category') }}" target="_parent"><img src="images/templatemo_logo.png" alt="Mini Social" /></a>
                </div> <!-- end of header -->

                <ul class="navigation">
                    <li><a href="{{ path('category') }}">Home<span class="ui_icon home"></span></a></li>
                    <li><a href="{{ path('product') }}">Product<span class="ui_icon aboutus"></span></a></li>
                    <li><a href="{{ path('category') }}">Category<span class="ui_icon services"></span></a></li>
                    <li><a href="{{ path('product') }}">.....<span class="ui_icon gallery"></span></a></li>
                    <li><a href="{{ path('category') }}">Contact Us<span class="ui_icon contactus"></span></a></li>
                </ul>
            </div> <!-- end of sidebar -->

            <div id="templatemo_main">
                <ul id="social_box">
                    <li><a href="http://www.facebook.com/templatemo"><img src="{{asset('/bundles/manageradmin/images/T/facebook.png')}}" alt="facebook" /></a></li>
                    <li><a href="http://www.facebook.com/templatemo"><img src="{{asset('/bundles/manageradmin/images/T/twitter.png')}}" alt="twitter" /></a></li>
                    <li><a href="http://www.facebook.com/templatemo"><img src="{{asset('/bundles/manageradmin/images/T/linkedin.png')}}" alt="linkin" /></a></li>
                    <li><a href="http://www.facebook.com/templatemo"><img src="{{asset('/bundles/manageradmin/images/T/technorati.png')}}" alt="technorati" /></a></li>
                    <li><a href="http://www.facebook.com/templatemo"><img src="{{asset('/bundles/manageradmin/images/T/myspace.png')}}" alt="myspace" /></a></li>                
                </ul>

                <div id="content">
                    {% block content %}
                    {% endblock %}
                   


                

                </div> <!-- end of content -->

                <div id="templatemo_footer">

                    Copyright Â© 2048 <a href="#">Your Company Name</a> | <a href="http://www.iwebsitetemplate.com" target="_parent">Website Templates</a> by <a href="http://www.templatemo.com" target="_parent">Free CSS Templates</a>

                </div> <!-- end of templatemo_footer -->

            </div> <!-- end of main -->
        </div>

      </body>
</html>